{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true,
    "node": true
  },
  "plugins": ["prettier", "import"],
  "extends": ["prettier", "eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": false, 
        "trailingComma": "all"
      }],
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error",
    "no-unused-vars": ["off", { "vars": "local" }]
  }
}